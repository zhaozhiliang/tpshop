<script>
    //底部导航上报
    var footNav = document.getElementById('footNav');
    if( footNav ){
        var footNavli = footNav.getElementsByTagName('li');
        for(var i = 0; i < footNavli.length; i++){
            var lia = footNavli[i].getElementsByTagName("a");
            lia[0].addEventListener("click", function(){
                var index = this.getAttribute('data-ind');
                if(!index){
                    return;
                }
                sbFootNav(index);
            })
        }
    }
    function sbFootNav(ind) {
        var statData = {
            'track_tag_name': '1009',
            'result': '',
            'page_content': ind,
            'track_type': '2',
            'message': ind
        };
        if (typeof(h5stat) == "object"){
            h5stat.shangbao(statData);
        }
    }
</script>    <div class="pop_appdown J_popAppdown" id="popAppdown" style='display: none;'>
