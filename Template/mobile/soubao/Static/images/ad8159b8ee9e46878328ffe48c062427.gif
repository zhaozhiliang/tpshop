	    //清除之前域名为.m.feiniu.com下的cookie
		(function (){
	        var keys = document.cookie.match(/[^ =;]+(?=\=)/g);
	        if (keys){
	            var path = "/";
	            var domain = "m.feiniu.com";
	            for(var i = keys.length; i--;){
	           		//document.cookie = keys[i]+'=0;expires=' + new Date(0).toUTCString();
	            	document.cookie = keys[i] + "=;path=" + path + ";" + (domain ? ("domain=" + domain + ";") : '') + "expires="+ new Date(0).toUTCString();
	            }
	        }
	    })();
	    
